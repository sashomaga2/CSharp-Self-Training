@model IEnumerable<eManager.Domain.Department>

@{
    ViewBag.Title = "All Department";
}

<h1>@ViewBag.Title</h1>

<ul class="list-group">
    @foreach(var department in Model){
        @Html.ActionLink(department.Name, "detail", "department", new { department.Id }, new { @class = "list-group-item" }) 
    }
</ul>

